    "def bubblesort(num):\n",
    "    n=len(num)\n",
    "    for i in range(n-2,-1,-1):\n",
    "        for j in range(0,i+1):\n",
    "            if num[j]>num[j+1]:\n",
    "                num[j],num[j+1] =num[j+1],num[j]\n",
    "                return  num\n",
    "print(bubblesort([152,625,85,96]))  "
   ]
